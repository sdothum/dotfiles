# Make_install
# ══════════════════════════════════════════════════════════════════════════════

# ....................................................................... quikey

python3 -m pipx install quikey

if [ ! -L $HOME/.local/share/quikey ] ;then
	cd stow >/dev/null
	stow quikey
fi

qk start

