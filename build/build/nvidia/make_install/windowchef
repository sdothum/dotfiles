# ................................................................... windowchef

sudo xbps-install -y libxcb-devel xcb-util-keysyms-devel xcb-util-wm-devel xcb-util-xrm-devel

cd /home/depot
exists -r windowchef || git clone https://github.com/tudorom/windowchef.git

cd windowchef
make
sudo make install

