[ $TRACE ] && CHILD="toggle/contrast $@" . $PARENT

# Windowchef
# ══════════════════════════════════════════════════════════════════════════════

# ......................................................................... Root

[ $(recipe theme/contrast) = dark ] && recipe theme/contrast light || recipe theme/contrast dark
chef theme $(recipe theme/contrast)
toggle picom on
restore panel

