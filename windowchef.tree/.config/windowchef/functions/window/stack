[ $TRACE ] && CHILD="window/stack $@" . $PARENT

# Windowchef
# ══════════════════════════════════════════════════════════════════════════════

# ........................................................... Manage group stack

# Usage: window stack <group>

ls -1 $GROUP:focus/$1  # return top of stack

