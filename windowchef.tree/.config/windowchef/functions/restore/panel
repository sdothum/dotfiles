[ $TRACE ] && CHILD="restore/panel $@" . $PARENT

# Windowchef
# ══════════════════════════════════════════════════════════════════════════════

# ................................................................ Restore panel

# Usage: restore panel [<panel>]

pkill -KILL -f "$WM/panel .*"

panel=$(recipe -e panel/format $1)
[ "$panel" ] || exit

draw panel $panel

# kak: filetype=sh
