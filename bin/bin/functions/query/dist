# ................................................................. Distribution

# using package managers..
if   alpine ;then echo alpine
elif arch   ;then echo artix
elif void   ;then echo nvidia   # asus/ryzen/nvidia sff build
else              echo unknown  # new distro lacking test/distro
fi

