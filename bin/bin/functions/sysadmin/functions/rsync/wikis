# ................................................................ Rsync wrapper

ifno 'recover thedarnedestthing.com:/vimwiki' && exit
trace -- "thedarnedestthing.com:/vimwiki => $(hostname)"
rsync -r --delete thedarnedestthing.com:/srv/http/thedarnedestthing.com/application/public/vimwiki $HOME/

