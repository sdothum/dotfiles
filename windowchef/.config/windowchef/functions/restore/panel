[ $TRACE ] && CHILD="restore/panel $@" . $PARENT

# Windowchef
# ══════════════════════════════════════════════════════════════════════════════

# ................................................................ Restore panel

pkill -KILL -f "panel .*"

[ -e $SESSION/panel ] || exit
panel=$(cat $SESSION/panel)
draw panel $panel

