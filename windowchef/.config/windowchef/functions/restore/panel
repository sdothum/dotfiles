[ $TRACE ] && CHILD="restore/panel $@" . $PARENT

# Windowchef
# ══════════════════════════════════════════════════════════════════════════════

# ................................................................ Restore panel

# Usage: restore panel [<panel>]

pkill -KILL -f "$WM/panel .*"
[ $1 ] || recipe -q panel/format || exit

draw panel $(recipe -e panel/format $1)

# kak: filetype=sh
