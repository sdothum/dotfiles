[ $TRACE ] && CHILD="window/stack $@" . $PARENT

# Windowchef
# ══════════════════════════════════════════════════════════════════════════════

# ........................................................... Manage group stack

# Usage: window stack <group>

cat $WME:group:$1:focus  # return top of stack

