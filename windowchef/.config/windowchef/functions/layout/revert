[ $TRACE ] && CHILD="layout/revert $@" . $PARENT

# Windowchef
# ══════════════════════════════════════════════════════════════════════════════

# ................................................................ Revert layout

for i in $(chef window ids) ;do
	window revert $i
done

waitron window_focus $winid
notify 2bwm "layout revert <b>ALL</b>"

# kak: filetype=sh
